{
    "configurations": [
    {
        "name": "Launch with routes",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "args": [
            "./example/config_routes",
            "--r=/api/some/{id}:testdata/someresponse.json",
            "--r=/api/soap:testdata/someresponse.xml"
        ],
        "program": "${workspaceFolder}"
    },
    {
        "name": "Launch with config",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "args": [
            "./example/config_routes",
            "--c=tmp/config.yaml"
        ],
        "program": "${workspaceFolder}"
    },
    ]
}